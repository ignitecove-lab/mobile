{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ASSET_BUNDLE_PATTERNS": "assets/**"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ASSET_BUNDLE_PATTERNS": "assets/**"
      }
    },
    "production": {
    "autoIncrement": true,
      "env": {
        "ASSET_BUNDLE_PATTERNS": "assets/**"
      }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "ignitecove-play-console.json",
        "track": "internal"
      }
    }
  }
}
